{
  "name": "commit-book",
  "main": "./lib/commit-book",
  "version": "0.2.0",
  "description": "Atom plugin to list your commits with handy features like search, sort, filter.",
  "keywords": [
    "git",
    "commit",
    "book",
    "github"
  ],
  "activationCommands": {
    "atom-workspace": "commit-book:toggle"
  },
  "repository": "https://github.com/gauravchl/commit-book",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-babel6-transpiler": "^1.2.0",
    "dugite": "^1.78.0",
    "moment": "^2.22.2",
    "node-emoji": "^1.8.1",
    "react": "^16.5.2",
    "react-date-range": "^1.0.0-beta",
    "react-dom": "^16.5.2",
    "react-gh-like-diff": "^1.0.0",
    "react-table": "^6.8.6"
  },
  "atomTranspilers": [
    {
      "glob": "**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "setBabelEnv": true,
        "babel": {
          "presets": [
            "es2015",
            "react"
          ],
          "babelrc": false
        }
      }
    }
  ]
}
