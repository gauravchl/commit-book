{
  "name": "commit-book",
  "main": "./lib/commit-book",
  "version": "0.0.0",
  "description": "A short description of your package",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": "commit-book:toggle"
  },
  "repository": "https://github.com/atom/commit-book",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "atom-babel6-transpiler": "^1.2.0",
    "dugite": "^1.78.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  },
  "atomTranspilers": [
    {
      "glob": "**/*.js",
      "transpiler": "atom-babel6-transpiler",
      "options": {
        "babel": {
          "presets": [
            "es2015",
            "react"
          ],
          "babelrc": false
        }
      }
    }
  ]
}
